</div>

<script>
    var applicants = [
        <% @applicants.each do |applicant| %>
        {   title: '<%=applicant.first_name%> <%=applicant.last_name %>',
            description: '<%=applicant.uin%>',
            url: "/admin/applications/<%=applicant.id %>",
            image: "http://pluspng.com/img-png/user-png-icon-male-user-icon-512.png"
        },
        <% end %>
    ];

    $('.ui.search').search({
        source : applicants,
        searchFields   : [
            'title', 'description'
        ],
    })
</script>